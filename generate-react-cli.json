{
	"usesTypeScript": true,
	"usesCssModule": false,
	"cssPreprocessor": "scss",
	"testLibrary": "Testing Library",
	"component": {
		"default": {
			"customTemplates": {
				"component": "templates/default/TemplateName.component.tsx",
				"style": "templates/default/TemplateName.styles.scss"
			},
			"path": "src/components",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false
		},
		"route": {
			"customTemplates": {
				"component": "templates/default/TemplateName.route.tsx",
				"style": "templates/default/TemplateName.styles.scss"
			},
			"path": "src/routes",
			"withStyle": true,
			"withTest": false,
			"withStory": false,
			"withLazy": false
		}
	}
}
